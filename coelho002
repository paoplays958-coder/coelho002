local coelholib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

local win = coelholib:CreateWindow("Coelho Hub v0.2 | by paozin")

-- Aba Menu/Home
local home = win:CreateTab("Menu", 4483362458)

home:CreateLabel("Bem-vindo ao Coelho Hub!")
home:CreateLabel("Versão: 0.2")
home:CreateButton("Ir para o Discord", function()
    setclipboard("https://discord.gg/pVsWE7WszB")
    coelholib:Notification("Coelho Hub", "Link do Discord copiado!", "Ok")
end)
home:CreateLabel("Script feito por paoplays958-coder")

-- Aba Fly
local fly = win:CreateTab("Fly", 4483362458)
local flying = false
local flyspeed = 50
fly:CreateToggle("Ativar Fly", function(state)
    flying = state
    local plr = game.Players.LocalPlayer
    local char = plr.Character or plr.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    if flying then
        local bv = Instance.new("BodyVelocity", hrp)
        bv.Name = "CoelhoFly"
        bv.MaxForce = Vector3.new(1e5, 1e5, 1e5)
        bv.Velocity = Vector3.new()
        local conn; conn = game:GetService("RunService").Heartbeat:Connect(function()
            if not flying or not bv.Parent then conn:Disconnect() end
            local cam = workspace.CurrentCamera
            local dir = Vector3.new()
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then dir = dir + cam.CFrame.LookVector end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then dir = dir - cam.CFrame.LookVector end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then dir = dir - cam.CFrame.RightVector end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then dir = dir + cam.CFrame.RightVector end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then dir = dir + Vector3.new(0,1,0) end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftShift) then dir = dir - Vector3.new(0,1,0) end
            if dir.Magnitude > 0 then dir = dir.Unit end
            bv.Velocity = dir * flyspeed
        end)
    else
        if hrp:FindFirstChild("CoelhoFly") then hrp.CoelhoFly:Destroy() end
    end
end)
fly:CreateSlider("Velocidade do Fly", 10, 200, 50, function(val)
    flyspeed = val
end)

-- Aba Speed
local speed = win:CreateTab("Speed", 4483362458)
local speedon = false
local walkval = 16
speed:CreateToggle("Ativar Speed", function(state)
    speedon = state
    local h = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h then
        h.WalkSpeed = state and walkval or 16
    end
end)
speed:CreateSlider("WalkSpeed", 16, 200, 16, function(v)
    walkval = v
    local h = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h and speedon then
        h.WalkSpeed = walkval
    end
end)

-- Aba Jump
local jump = win:CreateTab("Jump", 4483362458)
local jumpon = false
local jumpval = 50
jump:CreateToggle("Ativar Jump", function(state)
    jumpon = state
    local h = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h then
        h.JumpPower = state and jumpval or 50
    end
end)
jump:CreateSlider("JumpPower", 50, 300, 50, function(v)
    jumpval = v
    local h = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h and jumpon then
        h.JumpPower = jumpval
    end
end)

-- Aba Click TP
local tp = win:CreateTab("Click TP", 4483362458)
local toolActive = false
tp:CreateButton("Pegar Click TP", function()
    if toolActive then return end
    toolActive = true
    local tool = Instance.new("Tool")
    tool.RequiresHandle = true
    tool.Name = "Click TP"
    local handle = Instance.new("Part")
    handle.Size = Vector3.new(1,1,2)
    handle.Transparency = 1
    handle.Name = "Handle"
    handle.Parent = tool
    tool.Activated:Connect(function()
        local mouse = game.Players.LocalPlayer:GetMouse()
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = CFrame.new(mouse.Hit.Position) + Vector3.new(0,3,0)
        end
    end)
    tool.Parent = game.Players.LocalPlayer.Backpack
    coelholib:Notification("Coelho Hub", "Ferramenta Click TP adicionada no inventário!", "OK")
end)
tp:CreateLabel("Equipe a ferramenta e clique para teleportar onde quiser.")
